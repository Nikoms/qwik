{% block field_row %}
    <div class="control-group qwik-form-form-field">
        {{ form_label(form, null, {'label_attr': {'class': 'control-label' }}) }}
        <div class="controls">
            {{ form_widget(form, {'attr': {'class': 'input-xlarge' }}) }}
            <span style="display:none;" class="help-inline qwik-form-field-message">{{ form_errors(form) }}</span>
        </div>
    </div>
{% endblock field_row %}