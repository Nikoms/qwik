<meta charset="utf-8" />
<title>{% block title %}{{ page.getSite().getTitle() | translate ~ ' - ' ~ page.getTitle()|translate }}{% endblock %}</title>
<meta name="Description" content="{{ page.getConfig().get('meta.description') | translate }}">
<meta name="Keywords" content="{{ page.getConfig().get('meta.keywords') | translate }}">
<meta name="viewport" content="width=device-width">
<meta name="author" content="Qwik">

<script src="{{ asset('/qwik/js/jquery.js') }}"></script>
{% for file in app['qwik.module'].getAssets(page, 'css') %}
    <link href="{{ asset(file.getPath()) }}" rel="stylesheet" media="all">
{% endfor %}
