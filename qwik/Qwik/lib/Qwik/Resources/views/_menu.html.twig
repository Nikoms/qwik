<ul class="qwik-menu">
    {% for menuPage in app['qwik.page.service'].getAllPages(page.getSite()) %}
        {% if menuPage.isHidden() == false %}
            <li>
                <a href="{{ pathTo(menuPage.getUrl()) }}"{% if page.getUrl() == menuPage.getUrl() %} class="selected"{% endif %}>{{ menuPage.getTitle() | translate }}</a>
            </li>
        {% endif %}
    {% endfor %}

</ul>
